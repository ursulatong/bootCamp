// if x = 10, return r = 2*4*6*8*10
// if x = 6, 2*4*6
// if x = 20, 2*4*...*20
// if x = 19, 2*4*...*18
// where x>=2

public class Testing2 {

  // if x = 10, print 2,4,6,8,10
  public static void method() {

    for (int i = 2; i <= 10; i++) {
      if (i % 2 == 0) {
        System.out.println(i);
      }
    }
  }

  public static void main(String[] args) {

    Testing2.method();
  }
}
